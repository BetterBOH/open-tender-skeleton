{
  "name": "open-tender-skeleton",
  "version": "0.0.1",
  "description": "Skeletal React components for Open Tender client applications.",
  "repository": "https://github.com/BetterBOH/open-tender-skeleton",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=4.3.0"
  },
  "main": "dist/index.js",
  "author": "Sanctuary Computer <dev@sanctuary.computer> (https://www.sanctuary.computer/)",
  "license": "MIT",
  "scripts": {
    "build": "BABEL_ENV=production node_modules/.bin/babel --out-dir='dist' src --copy-files --source-maps",
    "start": "BABEL_ENV=production node_modules/.bin/babel -w --out-dir='dist' src --copy-files --source-maps",
    "sb": "start-storybook -p 9001 -c .storybook",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest"
  },
  "prettier": {
    "useTabs": false,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "jsxBracketSameLine": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "verbose": true
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@storybook/addon-a11y": "^4.1.4",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-notes": "^4.1.4",
    "@storybook/addon-options": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "basement": "git+https://github.com/sanctuarycomputer/basement.git",
    "css-loader": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.5.0",
    "brandibble-redux": "git+https://git@github.com/BetterBOH/brandibble-redux.git#7ecbee0daf8b65dbef3c4d15882930a3ed2f83a1",
    "connected-react-router": "^6.1.0",
    "currency.js": "^1.2.1",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "luxon": "^1.11.1",
    "mapbox-gl": "^0.52.0",
    "node-polyglot": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.3.0",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "uuid": "^3.3.2"
  }
}
