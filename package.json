{
  "name": "open-tender-skeleton",
  "version": "0.0.5",
  "description": "Skeletal React components for Open Tender client applications.",
  "repository": "https://github.com/BetterBOH/open-tender-skeleton",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=4.3.0"
  },
  "main": "dist/index.js",
  "author": "Sanctuary Computer <dev@sanctuary.computer> (https://www.sanctuary.computer/)",
  "license": "MIT",
  "scripts": {
    "build": "BABEL_ENV=production node_modules/.bin/babel --out-dir='dist' src --copy-files --source-maps",
    "build:watch": "BABEL_ENV=production node_modules/.bin/babel -w --verbose --out-dir='dist' src --copy-files --source-maps",
    "start": "./node_modules/.bin/concurrently -k npm:build:watch npm:lint:watch",
    "sb": "start-storybook -p 9001 -c .storybook",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "node_modules/.bin/eslint src",
    "lint:watch": "node_modules/.bin/esw src --color -w --changed --quiet",
    "test": "jest",
    "prepare": "BABEL_ENV=production node_modules/.bin/babel --out-dir='dist' src --copy-files --source-maps"
  },
  "prettier": {
    "useTabs": false,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "jsxBracketSameLine": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "verbose": true
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-notes": "^5.0.6",
    "@storybook/addon-options": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-watch": "^5.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.5.0",
    "@sanctucompu/basement": "^1.0.0",
    "@sentry/browser": "^5.3.0",
    "brandibble-redux": "git+https://git@github.com/BetterBOH/brandibble-redux.git#d47fadf56b1b146eca907aae3895dcfc83c00325",
    "currency.js": "^1.2.1",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "luxon": "^1.11.4",
    "mapbox-gl": "^0.52.0",
    "node-polyglot": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-datepicker": "^2.4.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.11",
    "react-select": "^2.3.0",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "uuid": "^3.3.2",
    "what-input": "^5.2.1"
  }
}
