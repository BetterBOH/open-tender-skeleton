{
  "name": "open-tender-skeleton",
  "version": "0.0.1",
  "description": "Skeletal React components for Open Tender client applications.",
  "repository": "https://github.com/BetterBOH/open-tender-skeleton",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=4.3.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepare": "rollup --config",
    "build": "rollup --config"
  },
  "prettier": {
    "useTabs": false,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "jsxBracketSameLine": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Sanctuary Computer <dev@sanctuary.computer> (https://www.sanctuary.computer/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "2.0.0-alpha.20",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rollup": "0.68.0",
    "rollup-plugin-babel": "^4.1.0-1",
    "rollup-plugin-collect-sass": "^1.0.9",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.6.3"
  }
}
